#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../appium-ios-driver/bin/xcode-iwd.sh" "$@"
else 
  exec bash  "$basedir/../appium-ios-driver/bin/xcode-iwd.sh" "$@"
fi
